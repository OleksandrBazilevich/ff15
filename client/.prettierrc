{
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "app",
    "pages",
    "widgets",
    "features",
    "entities",
    "shared",
    "../",
    "./"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
